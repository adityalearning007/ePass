@model IEnumerable<ePass.Passenger>

@{
    ViewBag.Title = "PassengerStatus";
}

@if (Model.Count() > 0)
{
    <table class="table table-striped">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Source</th>
            <th>Destination</th>
            <th>Adhar Card</th>
            <th>Status</th>
        </tr>
        <tr>
            @foreach (var item in Model)
            {
                <td>@item.firstName</td>
                <td>@item.LastName</td>
                <td>@item.SourcePlace</td>
                <td>@item.DestinationPlace</td>
                <td>@item.AdharCard</td>
                <td>@item.Status</td>
            }
        </tr>
    </table>
}
else
{ <h3>Not Found</h3>}

