@model IEnumerable<ePass.Passenger>

@{
    ViewBag.Title = "PassengerStatus";
}

@if (Model.Count()>0)
{
<table>
    <tr>
        <td>Status</td>
    </tr>
    <tr>
        @foreach (var item in Model)
        {
        <td>@item.Status</td>
        }
    </tr>
</table>
}
else { <h3>Not Found</h3>}

